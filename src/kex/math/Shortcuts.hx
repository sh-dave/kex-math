inline function fv2( x: FastFloat, y: FastFloat ) : FastVector2 {
    return new FastVector2(x, y);
}

inline function fv3( x: FastFloat, y: FastFloat, z: FastFloat ) : FastVector3 {
    return new FastVector3(x, y, z);
}

inline function fv4( x: FastFloat, y: FastFloat, z: FastFloat, w: FastFloat ) : FastVector4 {
    return new FastVector4(x, y, z, w);
}
